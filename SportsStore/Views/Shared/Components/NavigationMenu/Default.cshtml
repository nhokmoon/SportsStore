
<ul class="list-group">
    <li class="list-group-item">
        <a class="btn btn-block" asp-action="List"
           asp-controller="Product" asp-route-category="">Home</a>
    </li>
    @foreach (string category in Model)
    {
        <li class="list-group-item"> <a asp-action="List"
           asp-controller="Product" asp-route-category="@category">
            @category </a>
        </li>
    }
</ul>